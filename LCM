import java.util.Scanner;

public class LCM {
public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
	 System.out.print("Enter two numbers: ");
        int a = input.nextInt();
	 int b = input.nextInt();

        int lcm = (a * b) / gcd(a, b);
	System.out.println("LCM is: " + lcm);
    }
    static int gcd(int x, int y) {
        while (y != 0) {
	 int temp = y;
            y = x % y;
	     x = temp;
        }


